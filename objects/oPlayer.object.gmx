speed = 0

if keyboard_check( vk_shift ) { Walk = 5 Sprinting = true } else { Walk = 4 Sprinting = false}

if instance_number(oPlayer) &gt; 1 { with ( instance_nearest(x,y,oPlayer) ) { instance_destroy() } }


if instance_exists(oWallMaker) {
    visible = false
} else {
    visible = true
    ScWalk()
}

if Up or Down or Left or Right { if Sprinting = false {image_speed = .25} else {image_speed = .5} } else { image_speed = 0 }
//if Walking = true { image_speed = .25 } else { image_speed = 0 }
if room = rSpace { speed = 0 visible = false } else { visible = true }

//Pickup Guns!

if HoldingGun = false {

    if keyboard_check_pressed(ord('E')) and distance_to_object(oWeapon) &lt; 20 {
        with (instance_nearest(x,y,oWeapon)) { PickedUp = true }
        HoldingGun = true
    }

} else {

    if keyboard_check_pressed(ord('Q')) { HoldingGun = false with (oWeapon) { PickedUp = false } }

}

// eating/babies (not eating babies)
food = instance_nearest(x,y,oDead)
if place_meeting(x,y,food) { Stomach += 2 with (food) { instance_destroy() } }

if keyboard_check(vk_space) and Stomach &gt; 0 { instance_create(x,y,oWorker) Stomach -= 1 }

if mouse_check_button_pressed(mb_right) { instance_create(mouse_x,mouse_y,oCall) }

//eat your children

Children = instance_nearest(x,y,oWorker) 

if distance_to_object(Children) &lt; 40 and keyboard_check(ord('X')) { 

    with (Children) { instance_destroy() }    
    
    Stomach += 1

}

//make sure i always have a baby

if Stomach &lt; 1 and instance_number(oWorker) &lt; 1 { Stomach += 1 }

//throwing

if mouse_check_button_pressed(mb_left) {
    if instance_exists(oWorker) {
    
        Throw = instance_nearest(x,y,oWorker)
        if distance_to_object(Throw) &lt; 25 {
            TarDis = distance_to_point(mouse_x,mouse_y)
            if TarDis &gt; 400 { Tar = 400 } else { Tar = TarDis }
            with (Throw) {
                direction = point_direction(x,y,mouse_x,mouse_y)
                speed = 10
                thrown = true
                Tar = other.Tar
                Derp = point_direction(x,y,mouse_x,mouse_y)
                TargetX = x + lengthdir_y(Tar,Derp)
                TargetY = y + lengthdir_y(Tar,Derp)  
                if Tar &lt; 400 { TargetX = mouse_x TargetY = mouse_y } else { TargetX = global.X TargetY = global.Y }        
            }
            
        }
    
    }
}







